generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Livro {
  id        Int      @id @default(autoincrement())
  titulo    String
  isbn      String   @unique
  editora   String?
  paginas   Int?
  ano       Int?
  autores   Json?
  createdAt DateTime @default(now())
}